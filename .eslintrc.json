{
    "extends": "airbnb-base",
    "env": {
        "browser": true
    },
    "rules": {
        "semi": ["error", "never", { "beforeStatementContinuationChars": "always"}],
        "no-console": "off",
        "no-restricted-syntax": "off",
        "comma-dangle": ["error", "always-multiline"]

      }
}